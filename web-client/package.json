{
  "name": "web-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test": "npm run test:unit && npm run test:e2e",
    "coverage:unit": "vitest --run --coverage",
    "dev": "vite",
    "build-only": "vite build",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src tests --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint src tests --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "preview": "vite preview",
    "build": "run-p \"build-only {@}\" --",
    "build_with_maps": "VITE_BUILD_SOURCEMAP=true run-p \"build-only {@}\" --",
    "format": "prettier --write src/",
    "pw:codegen": "playwright codegen",
    "pw:report": "playwright show-report",
    "prepare": "husky"
  },
  "dependencies": {
    "@apache-arrow/esnext-esm": "^17.0.0",
    "@apache-arrow/ts": "^17.0.0",
    "@deck.gl/core": "^9.2.2",
    "@deck.gl/layers": "^9.2.2",
    "@duckdb/duckdb-wasm": "^1.30.0",
    "@primeuix/themes": "^1.2.5",
    "@turf/turf": "^7.2.0",
    "@types/jest": "^29.5.13",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext@^3.6.5",
    "@vueuse/core": "^11.1.0",
    "apache-arrow": "^17.0.0",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.4",
    "colormap": "^2.3.2",
    "dexie": "^4.2.1",
    "duckdb": "^1.4.2",
    "echarts": "^5.6.0",
    "echarts-gl": "^2.0.9",
    "events": "^3.3.0",
    "highlight.js": "^11.11.1",
    "intro.js": "^8.3.2",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.9.2",
    "mitt": "^3.0.1",
    "ol": "^10.7.0",
    "ol-contextmenu": "^5.5.0",
    "ol-ext": "^4.0.36",
    "ol-geocoder": "^4.3.3",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^4.0.1",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "sharp": "^0.34.3",
    "shpjs": "^6.1.0",
    "vue": "^3.5.24",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.6.3",
    "vue3-openlayers": "^12.2.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/colormap": "^2.3.4",
    "@types/intro.js": "^5.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.5",
    "@types/proj4": "^2.5.5",
    "@types/shapefile": "^0.6.4",
    "@types/shpjs": "^3.4.7",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/compiler-sfc": "^3.5.20",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.0",
    "lint-staged": "^16.2.6",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "typescript": "~5.6.2",
    "vite": "^6.0.11",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vue-devtools": "^7.6.4",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.1.6"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "bash -c 'ESLINT_USE_FLAT_CONFIG=false eslint --fix \"$@\"' --",
      "prettier --write"
    ]
  },
  "//overrides": "jspdf override fixes GHSA-f8cm-6447-x5h2 (Local File Inclusion vulnerability in jspdf <=3.0.4)",
  "overrides": {
    "jspdf": "^4.0.0"
  }
}
