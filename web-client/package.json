{
  "name": "web-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p \"build-only {@}\" --",
    "build_with_maps": "VITE_BUILD_SOURCEMAP=true run-p \"build-only {@}\" --",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-only": "VITE_APP_VERSION=$(node -p \"require('./package.json').version\") vite build",
    "dev": "VITE_APP_VERSION=$(node -p \"require('./package.json').version\") vite",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@deck.gl/core": "^8.9.35",
    "@deck.gl/layers": "^8.9.35",
    "@types/jest": "^29.5.12",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext@^3.3.0",
    "@vue/compiler-sfc": "^3.4.21",
    "@vueuse/core": "^10.9.0",
    "buffer": "^6.0.3",
    "dexie": "^4.0.4",
    "events": "^3.3.0",
    "mitt": "^3.0.1",
    "ol": "^8.2.0",
    "ol-contextmenu": "^5.3.0",
    "ol-ext": "^4.0.17",
    "ol-geocoder": "^4.3.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.44.0",
    "vue": "^3.4.21",
    "vue-router": "^4.2.5",
    "vue3-openlayers": "^4.0.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@types/proj4": "^2.5.5",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "cypress": "^13.6.1",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.27"
  },
  "type": "module"
}
